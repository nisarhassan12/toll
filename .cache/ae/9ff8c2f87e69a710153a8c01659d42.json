n        name: '',\n        amount: '',\n        description: ''\n    }\n\n    onNameChange = (e) => {\n        const name = e.target.value\n        this.setState(() => ({name}))\n    }\n\n    onAmountChange = (e) => {\n        const amount = e.target.value\n    \n        if (!amount || amount.match(/^\\d{1,}(\\.\\d{0,2})?$/)) {\n          this.setState(() => ({ amount }))\n        }\n    }\n\n    onDescriptionChange = (e) => {\n        const description = e.target.value\n        this.setState(() => ({description}))\n    }\n\n    handleClear() {\n        this.setState(() => ({name: '', amount: '', description: ''}))\n    }\n\n    handleSubmit = (e) => {\n        e.preventDefault()\n        const {history, addItem} = this.props\n        addItem('expenses',this.state)\n        history.push('/expenses')\n    }\n\n    render() {\n        return (\n            <React.Fragment>\n                <form className='form' onSubmit={this.handleSubmit}>\n                    <h1 className='heading-primary'>Add Expense</h1>\n                    <input \n                        type='text'\n                        value={this.state.name}\n                        name='name'\n                        placeholder='Name...'\n                        onChange={this.onNameChange}\n                        className='form__name'\n                        required    \n                    />\n                    <input\n                        type=\"text\"\n                        value={this.state.amount}\n                        name='amount'\n                        placeholder='Amount...'\n                        onChange={this.onAmountChange}\n                        className=\"form__amount\"\n                        required\n                    />\n                    <input\n                        type=\"text\"\n                        value={this.state.description}\n                        name='description'\n                        placeholder='Description...'\n                        onChange={this.onDescriptionChange}\n                        className=\"form__description\"\n                    />\n                    <button\n                        className='form__button btn'\n                    >\n                        Add Expense\n                    </button>\n                </form>\n            </React.Fragment>\n        )\n    }\n\n}\n\n\nexport default Form\n"},"lineCount":null}},"hash":"7dc5b316de45c94042c094a4e8c47677","cacheData":{"env":{}}}